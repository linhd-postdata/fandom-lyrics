[
    {
        "author": "Marcela Mosqueda",
        "title": "No Puedo Tenerte",
        "text": [
            [
                "Lo siento, vuelves a decir",
                "Regresas a mi",
                "Trato de resistir",
                "si dices lo siento"
            ],
            [
                "Perdona, vuelves a decir",
                "Yo me entrego a ti",
                "Y al final debo salir",
                "Diciendo perdona"
            ],
            [
                "No puedo tenerte",
                "río y luego vuelvo a llorar",
                "Nunca se si tu vas a regresar",
                "ya no puedo más, esperar"
            ],
            [
                "Te amo, vuelves a decir",
                "Quiero resistir",
                "No puedo más que decir",
                "que te amo, te amo"
            ],
            [
                "No puedo tenerte",
                "río y luego vuelvo a llorar",
                "Nunca se si tu vas a regresar",
                "ya no puedo más, esperar"
            ]
        ]
    }
]