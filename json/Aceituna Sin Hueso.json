    {
        "author": "Aceituna Sin Hueso",
        "title": "Marginados.Com",
        "text": [
            [
                "Compro boletos de vuelta",
                "a una ciudad que desnudo,",
                "donde moran adoquines,",
                "donde duelen los saludos"
            ],
            [
                "Muecas, guiños, entrepiernas,",
                "rostros, salud sin condón,",
                "comercio de soledades,",
                "marginales com"
            ],
            [
                "Unos versos estampados",
                "en ranuras, en ladrillos,",
                "un perro, un hombre, una cruz,",
                "un señor que vende anillos"
            ],
            [
                "Y de regreso a los techos",
                "de lloviznas y granizos,",
                "bebo café entre los guiños,",
                "pago un peso y me bautizo",
                "En una urdimbre de nombres,",
                "en un retiro sin juicio,",
                "en una fiesta de pobres,",
                "en un camino sin quicio"
            ],
            [
                "",
                "Muecas, guiños, entrepiernas,",
                "rostros, salud sin condón,",
                "comercio de soledades,",
                "marginales com"
            ]
        ]
    }
]