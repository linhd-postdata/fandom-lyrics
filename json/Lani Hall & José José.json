[
    {
        "author": "Lani Hall & José José",
        "title": "Te quiero asi",
        "text": [
            [
                "Te quiero asi",
                "Sin mas ni mas",
                "Unas veces dominante",
                "Otras veces soñadora",
                "Te quiero amante"
            ],
            [
                "Te quiero asi",
                "Sin mas ni mas",
                "Tan violento",
                "Como el viento",
                "Pero dulce como un beso",
                "A la hora de amar"
            ],
            [
                "Te quiero asi",
                "Tu conmigo y yo para ti",
                "Inventando un cielo",
                "Color caramelo",
                "Vivir por vivir",
                "Amar por amar",
                "Mas que amor es ir a navergar",
                "Donde la aventura nos quiera llevar",
                "Que mas nos da"
            ],
            [
                "Te quiero asi",
                "Tan natural",
                "Como el aire",
                "Como el trigo",
                "Con caricias de mujer",
                "Y piel de nino"
            ],
            [
                "Te quiero asi",
                "Tan natural",
                "Como el agua en que me miro",
                "Que mas da rico mendigo",
                "Te quiero por ti"
            ],
            [
                "Te quiero asi",
                "Tu conmigo y yo para ti",
                "Inventando un cielo",
                "Color caramelo",
                "Vivir por vivir",
                "Amar por amar",
                "Mas que amor es ir a navergar",
                "Donde la aventura nos quiera llevar",
                "Que mas nos da"
            ],
            [
                "Te quiero asi",
                "Tu conmigo y yo para ti",
                "Inventando un cielo",
                "Color caramelo",
                "Vivir por vivir",
                "Amar por amar",
                "Mas que amor es ir a navergar",
                "Donde la aventura nos quiera llevar",
                "Que mas nos da"
            ],
            [
                "Te quiero asi",
                "Sin mas ni mas"
            ]
        ]
    }
]