[
    {
        "author": "Ronan Portela",
        "title": "¿Qué Te Dicen Todos?",
        "text": [
            [
                "Olvidate las disculpas",
                "No me hablés más",
                "Revisá tu pensamiento",
                "Respirá"
            ],
            [
                "No me muestres tus pecados",
                "Tan natural",
                "El silencio te acompaña",
                "Es real"
            ],
            [
                "Yo ya sé que no te gusta",
                "Mi religión",
                "Vos callate y relajate",
                "No sabés dónde estás"
            ],
            [
                "Despertate y acordate",
                "No hay más qué hablar",
                "El lenguaje te marea",
                "Descansá"
            ],
            [
                "Todos te dicen",
                "Que las palabras no existen",
                "Todos te dicen",
                "Que no existen..."
            ]
        ]
    },
    {
        "author": "Ronan Portela",
        "title": "What Do You Know?",
        "text": [
            [
                "¡Callate vos, ¿qué sabés vos?!",
                "Yo sé mucho"
            ],
            [
                "(Qué sabés vos...",
                "Qué sabés vos...)"
            ]
        ]
    }
]