[
    {
        "author": "Victor Manuel Lujan",
        "title": "La Ultima Cancion",
        "text": [
            [
                "esta es la ultima cancion",
                "que escribo para ti"
            ],
            [
                "me canse de vivir de sin sentido",
                "de pensar solo en ti",
                "si manana me encuentras tal vez",
                "yendo con otra",
                "y tu me ves hazte la cuenta",
                "que para ti no soy aquel"
            ],
            [
                "y ahora tu que me hiciste llorar",
                "tendras que recordar",
                "que fui tuyo en el alma",
                "y perdiste",
                "tu oportunidad"
            ],
            [
                "las canciones mas lindas de amor",
                "que escribi para ti",
                "pensando en ti"
            ],
            [
                "confieso iguales a aquellas",
                "ya no escucharas",
                "ya manana se que esta cancion",
                "tendras que escuchar en la radio sonar",
                "pensaras que tu orgullo maldito",
                "me izo llorar"
            ],
            [
                "pero no llores",
                "y siente por dentro",
                "que duele el amor"
            ],
            [
                "entonces vas a ver",
                "lo que vale un gran amor",
                "y entonces lloraras y de mi",
                "te acordaras"
            ],
            [
                "entonces vas a ver",
                "lo que vale un gran amor",
                "y entonces lloraras",
                "y de mi te acordaras"
            ],
            [
                "esta es la ultima cancion",
                "que escribo para ti"
            ],
            [
                "me canse de vivir sin sentido",
                "de pensar solo en ti",
                "si manana me encuentras tal vez",
                "yendo con otra y tu me vez",
                "hazte la cuenta que para ti",
                "no soy aquel"
            ],
            [
                "hazte la cuenta que para ti",
                "no soy aquel"
            ]
        ]
    }
