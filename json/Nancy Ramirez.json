[
    {
        "author": "Nancy Ramirez",
        "title": "Sin Ti No Puedo Vivir",
        "text": [
            [
                "Puede quedarse la tierra",
                "Sin luna y sin sol",
                "Puede quedarse los peces",
                "Sin agua en el mar",
                "Puede no brillar las estrellas",
                "Puede no brillar mas el sol",
                "Pero señor yo sin ti no puedo vivir"
            ],
            [
                "Puede quedarsen los campos",
                "Sin lluvia llegar",
                "Puede quedarsen las plantas",
                "Sin flores hechar",
                "Pude acabarse la tierra",
                "Que se seque el agua del mar",
                "Pero señor yo sin ti no puedo vivir"
            ],
            [
                "Tu eres el todo de mi vida",
                "Eres la fuerza de mi ser",
                "Tu me das esa razon",
                "Para vivir para cantar",
                "Tu le das a mi corazon",
                "Un toque especial"
            ],
            [
                "Tu eres el manantial",
                "Donde mi vida puede saciar",
                "Esa sed infinita de amar"
            ],
            [
                "Pero señor yo sin ti no puedo vivir"
            ]
        ]
    }
]