[
    {
        "author": "Daniel Muñoz",
        "title": "Señor Cura",
        "text": [
            [
                "Perdóneme señor cura"
            ],
            [
                "Perdóneme señor cura",
                "Que traigo muchoh pecaoh",
                "La carga que traigo a cuestah",
                "Me tiene desesperao."
            ],
            [
                "El pecao máh simple",
                "Y máh sencillo",
                "A mi suegra le hice",
                "Cinco chiquilloh."
            ],
            [
                "Cinco chiquilloh ay sí",
                "A mih cuñáh",
                "La mayor ta paría",
                "La otra preñá."
            ],
            [
                "Déjalo pa callao",
                "No hay ni un pecao."
            ]
        ]
    }
]