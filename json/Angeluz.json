                "contra fieros destinos",
                "yo estare cerca de ti",
                "hasta la muerte hasta el fin"
            ],
            [
                "coro:"
            ],
            [
                "angel de luz",
                "enseñame como llegar",
                "al reino de la eternidad"
            ],
            [
                "angel de luz",
