[
    {
        "author": "Ivan Lins",
        "title": "Te Recuerdo Amanda",
        "text": [
            [
                "Te recuerdo Amanda",
                "La calle mojada",
                "Corriendo a la fabrica donde trabajaba Manuel"
            ],
            [
                "La sonrisa ancha, la lluvia en el pelo,",
                "No importaba nada",
                "Ibas a encontrarte con el,",
                "Con el, con el, con el, con el"
            ],
            [
                "Son cinco minutos",
                "La vida es eterna,",
                "En cinco minutos"
            ],
            [
                "Suena la sirena,",
                "De vuelta al trabajo",
                "Y tu caminando lo iluminas todo",
                "Los cinco minutos",
                "Te hacen florecer"
            ],
            [
                "Te recuerdo amanda",
                "La calle mojada",
                "Corriendo a la fabrica",
                "Donde trabajaba Manuel"
            ],
            [
                "La sonrisa ancha",
                "La lluvia en el pelo",
                "No importaba nada,",
                "Ibas a encontrarte con el,",
                "Con el, con el, con el, con el"
            ],
            [
                "Que partió a la sierra",
                "Que nunca hizo daño,",
                "Que partió a la sierra",
                "Y en cinco minutos,",
                "Quedó destrozado"
            ],
            [
                "Suenan las sirenas",
                "De vuelta al trabajo",
                "Muchos no volvieron",
                "Tampoco manuel"
            ],
            [
                "Te recuerdo Amanda,",
                "La calle mojada",
                "Corriendo a la fábrica,",
                "Donde trabajaba Manuel."
            ]
        ]
    }
]