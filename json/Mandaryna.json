[
    {
        "author": "Mandaryna",
        "title": "Esta Noche",
        "text": [
            [
                "Te quiero llenar de Alegría...",
                "Te quiero llenar de amor..",
                "Te quiero dar mis fantasías",
                "Mis noches, mi comprensión"
            ],
            [
                "Hacerte sonar..",
                "Y Hacerte vibrar..",
                "Como nadie te lo pudo dar"
            ],
            [
                "Y así tu sabrás",
                "Que mío serás",
                "Por siempre mi sol verás"
            ],
            [
                "Esta noches mi amor",
                "Esta noche te lo daré yo",
                "Te entregaré mi vida",
                "Llena de pasión"
            ],
            [
                "Esta noche soy yo",
                "Esta nocha te pido ser tú",
                "Llenos de alegría",
                "Sin ningun temor"
            ],
            [
                "Yo quiero que seas mi vida",
                "Yo quiero que seas mi paz",
                "Que me llenes de poesía",
                "Mil Besos y mucho más"
            ],
            [
                "Hacerme soñar",
                "Y hacerme vibrar",
                "Como nadie me lo pudo dar"
            ],
            [
                "Y así yo sabré",
                "Que tuya seré",
                "Por siempre por ti estaré"
            ]
        ]
    }
]