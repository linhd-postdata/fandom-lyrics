[
    {
        "author": "Aline Marin",
        "title": "Que Pedi, Que Recibi",
        "text": [
            [
                "Pedí carbón me diste luz",
                "Busque morir moriste en la cruz",
                "Quise sanar vendaste el corazón",
                "Sufrí dolor te hiciste canción"
            ],
            [
                "Pedí carbón me diste luz",
                "Busque morir moriste en la cruz",
                "Quise sanar vendaste el corazón",
                "Sufrí dolor te hiciste canción"
            ],
            [
                "Que pedí, que recibí",
                "Siempre das mas de lo que uno suele pedir",
                "Eres infinito das con amor",
                "Que pedí que recibí",
                "No detienes jamás",
                "Lo que hay en tu interior",
                "Que recibí"
            ],
            [
                "Ore por pan pusiste tu cuerpo",
                "En duda andaba tu fuiste lo cierto",
                "Pedí comer saciaste mi hambre",
                "Me augaba en vino",
                "Diste tu sangre"
            ],
            [
                "Estaba yo solo",
                "Estuviste a mi lado",
                "Busque sin hallar",
                "Halle lo buscado",
                "Andaba en tinieblas",
                "Fuiste tan tierno",
                "Era de nadie y me hiciste tu siervo"
            ],
            [
                "Que pedí, que recibí",
                "Siempre das mas de lo que uno suele pedir",
                "Eres infinito das con amor",
                "Que pedí que recibí",
                "No detienes jamás",
                "Lo que hay en tu interior",
                "Que pedí, que recibí",
                "Siempre das mas de lo que uno suele pedir",
                "Eres infinito das con amor",
                "Que pedí que recibí",
                "Tu no detienes jamás",
                "Lo que hay en tu interior",
                "Que recibí"
            ]
        ]
    }
]