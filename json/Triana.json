            ],
            [
                "Los árboles contaban",
                "Historias de otros mundos",
                "Con danzas expresivas",
                "Para un corazón sediento"
            ],
            [
                "Luminosa mañana",
                "Prendida de sufrimiento",
                "Hoy he visto la luz",
                "Que todos llevamos dentro"
            ]
        ]
    },
