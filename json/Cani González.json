[
    {
        "author": "Cani González",
        "title": "Llegó El Amor",
        "text": [
            [
                "Llegó el amor, mmmm",
                "Llegó el amor",
                "El sueño tan dulce de ayer"
            ],
            [
                "Feliz estoy, mmmm",
                "Pues sé que hoy (Pues sé que hoy)",
                "La llave del cielo hallé"
            ],
            [
                "Jamás sentí, mmmm",
                "La sensación",
                "De estrellas en el corazón"
            ],
            [
                "Por fin ha ocurrido ya",
                "Mi sueño se cumplió"
            ],
            [
                "Mmmm, mmmm",
                "Llegó el amor."
            ]
        ]
    }
]