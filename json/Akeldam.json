[
    {
        "author": "Akeldam",
        "title": "Nostalgia Oscura",
        "text": [
            [
                "Yo me encuentro en lo oscuro muy solo.",
                "nadie me escucha",
                "mi alma",
                "ya no puede.......",
                "sufrir...",
                "grito en silencio",
                "por siempre",
                "sufrire",
                "el refugio",
                "de mi dolor",
                "se encuentra",
                "en las sombras",
                "nooo......",
                "los vientos",
                "me alejan",
                "del camino",
                "de la luz..",
                "ciego",
                "yo me encuentro",
                "deseo",
                "ver.....",
                "nooo.......!!",
                "muestrame el camino",
                "que me siento perdido",
                "sangrando mi cuerpo",
                "me alejan de la vida",
                "las heridas se rasgan",
                "mi alma agoniza..."
            ],
            [
                "mis ilusiones se fueron marchitando",
                "noo!! porque!! yoo...",
                "padesco...de luz...de paz..",
                "en mi vida...",
                "yo me encuentro",
                "en lo oscuro",
                "muy solo",
                "nadie",
                "nadie me escucha",
                "mi alma ya no puede sufrir",
                "grito en silencio",
                "quiero vivir......"
            ]
        ]
    }
]