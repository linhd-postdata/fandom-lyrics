[
    {
        "author": "Eudora",
        "title": "Lirio",
        "text": [
            [
                "Busquemos en carro alado",
                "Busquemos detras del sol",
                "De cisnes blancos",
                "Sangre centellante",
                "En tu aislado corazon",
                "Mil petalos y un solo pu~al",
                "Dejame en el planeta",
                "Embriagador, suicida y auroral",
                "En el cual tu me succionas",
                "De tu cintura",
                "Venus en el mar de mis lamentos",
                "Mujer sonica",
                "Fortalecen con tus lagrimas"
            ],
            [
                "Al chico alado",
                "Un llanto estalla",
                "Entre mis viseras de metal"
            ],
            [
                "En mi interior",
                "Mil petalos y un lirio voraz",
                "Mi sangre como un virus beberas."
            ],
            [
                "Busquemos en tus colores",
                "Alla en el abismo",
                "Una solucion concentrada en esmeraldas",
                "Mis dias e inviernos grises",
                "Atraparas con tus abriles"
            ]
        ]
    }
]