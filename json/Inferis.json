[
    {
        "author": "Inferis",
        "title": "Tradición Y Sangre",
        "text": [
            [
                "Siente el hierro",
                "Sangre corre entre mis manos",
                "Mírame por vez final",
                "Es lo último que harás",
                "¡Viertan oro líquido!",
                "Sacia tu sed de poder",
                "El trueno estalla",
                "El monte habló",
                "Nuestro dios surge del fuego",
                "Siéntelo gritar!"
            ],
            [
                "Quédate",
                "Junto a mí",
                "Desvanece tu temor",
                "El viento",
                "me sigue",
                "y... Arauco es mas",
                "Fuerte que ayer"
            ],
            [
                "Se fuerte y tenaz",
                "Arma heroica",
                "Alzate en la gesta final"
            ],
            [
                "Fuego soy,",
                "Tradición y sangre en mi piel",
                "Es mi voz",
                "Reflejo de mi tierra y corazón",
                "El tiempo",
                "Dará la razón de mi acción",
                "Sígueme hoy",
                "Hermano que",
                "Cien veces venceré"
            ],
            [
                "No verás",
                "Miedo en mí",
                "Nunca más"
            ],
            [
                "...nunca más..."
            ],
            [
                "Sientes mi pueblo gritar?"
            ]
        ]
    }
]