[
    {
        "author": "Ivan Cattaneo",
        "title": "Cha Cha Che Guevara",
        "text": [
            [
                "Muchacho de fruta candita",
                "me gusta… me gustan las chicas",
                "caramba por la cumparsita",
                "me gusta… me gusta la vita"
            ],
            [
                "VITA UH! VITA UH!",
                "VITA UH! VITA UH!"
            ],
            [
                "Avana GORILLA-GUERRILLA",
                "los hombres barbudos kadillak",
                "tobacco de CASTRO ministro",
                "me gusta…me gusta 'l CALYPSO"
            ],
            [
                "CRISCO UH! CRISCO UH!",
                "CRISCO UH! CRISCO UH!"
            ],
            [
                "CUBA, CUBA, CUBA, CU",
                "CUBA, CUBA, CUBA",
                "CHA, CHA, CHA, CHA",
                "CHA, CHA, CHA",
                "CHE, CHE, CHE",
                "CHE, CHE, CHE GUEVARA"
            ],
            [
                "Ciquita, dai, besame mucho",
                "me gusta, me gusta muchacho",
                "te quiero REVOLVER del MAMBO",
                "corazon, BANANA, MOKAMBO"
            ],
            [
                "MAMBO UH! MAMBO UH!",
                "MAMBO UH! MAMBO UH!"
            ],
            [
                "Cigarro, GUEVARA tradido",
                "hurrà por l'amigo bandido",
                "tobacco de CASTRO ministro",
                "me gusta…me gusta 'l CALYPSO"
            ],
            [
                "CRISCO UH! CRISCO UH!",
                "CRISCO UH! CRISCO UH!"
            ],
            [
                "CUBA, CUBA, CUBA, CU",
                "CUBA, CUBA, CUBA",
                "CHA, CHA, CHA, CHA",
                "CHA, CHA, CHA",
                "CHE, CHE, CHE",
                "CHE, CHE, CHE GUEVARA"
            ]
        ]
    }
]