[
    {
        "author": "Sandra",
        "title": "Silencio A Mi Lado",
        "text": [
            [
                "Sola",
                "Una eternidad",
                "Piensas que he desaparecido",
                "Desde el vacio",
                "Silencio a mi lado",
                "Por ti"
            ],
            [
                "No puedo mas",
                "Estas tan cerca",
                "Pero donde estas"
            ]
        ]
    }
]