[
    {
        "author": "Ibiza Pareo",
        "title": "Árido Espejismo",
        "text": [
            [
                "Busco tu imagen en la arena",
                "Te veo en el reflejo del Sol",
                "Sigo tus huellas movedizas",
                "Mi cuerpo está lleno de sudor"
            ],
            [
                "Siento la inmensidad del aire",
                "El viento hace música en su baile",
                "El Sol juega con mi imaginación"
            ]
        ]
    },
    {
        "author": "Ibiza Pareo",
        "title": "Charup",
        "text": [
            [
                "Ven hacia mí",
                "Tus labios me llaman",
                "Me intento mover",
                "El ritmo me atrapa"
            ],
            [
                "Tus caderas…",
                "No lo puedo——— no, no"
            ],
            [
                "Quiero gritar",
                "De amor"
            ],
            [
                "Y las melodías a dónde fueron",
                "Muchos bombos, muchas líneas",
                "Dame un silbido de tu viento",
                "Dame un guitarrón",
                "Y esa dulce flauta que me viaje"
            ]
        ]
    },
    {
        "author": "Ibiza Pareo",
        "title": "Discoteca",
        "text": [
            [
                "Que el beat tome mi cuerpo",
                "Y la música",
                "Llene por dentro",
                "Quiero Viajar"
            ],
            [
                "En el sonido",
                "En la melodía",
                "Y en esta noche",
                "En la discoteca"
            ],
            [
                "Que el ritmo tome el control",
                "En todo mi ser",
                "Quiero entregarme de lleno",
                "Estallar de placer"
            ],
            [
                "En el sonido",
                "En la melodía",
                "Y en esta noche",
                "En la discoteca"
            ],
            [
                "En el sonido",
                "En la melodía",
                "Y en esta noche",
                "En la discoteca"
            ],
            [
                "Las luces giran",
                "Mis ojos vuelan",
                "Cada bombo",
                "Me libera",
                "Salgo de mí",
                "En comunión",
                "En el baile",
                "La pasión"
            ]
        ]
    },
    {
        "author": "Ibiza Pareo",
        "title": "Nuestro Amor Es Música",
        "text": [
            [
                "Mueve tu cuerpo",
                "Suelta tu voz",
                "Respira profundo",
                "Y mímate"
            ],
            [
                "Suspiro su olor",
                "Es bella y salvaje"
            ],
            [
                "Tú bailas, me bailas",
                "Música y sudor"
            ],
            [
                "Escucharte",
                "Escucharme",
                "Abrázame",
                "Y llévame"
            ],
            [
                "Es nuestro amor",
                "Es música",
                "Es nuestro amor",
                "Es música",
                "Es nuestro amor",
                "Es música",
                "Música",
                "Música",
                "Música",
                "Música"
            ]
        ]
    },
    {
        "author": "Ibiza Pareo",
        "title": "Ritmas",
        "text": [
            [
                "No sientas temor",
                "(No sientas temor)",
                "No sientas presión",
                "No sientas presión"
            ],
            [
                "El aburrimiento",
                "No es nuestro fuerte",
                "(Es nuestro fuerte, eh)"
            ],
            [
                "Mi casa está siempre abierta",
                "Yo también, también tendré miedo",
                "Nos adentramos sin darnos cuenta"
            ],
            [
                "Es la nube pasajera",
                "Aprendamos del tiempo",
                "Que nos lleve",
                "Donde con mirarnos baste",
                "Y sólo nuestro besos escuchemos"
            ],
            [
                "Yo estoy acá",
                "Y vos donde quieras estar",
                "Yo estoy acá",
                "Sin vos",
                "Y vos donde quieras estar"
            ],
            [
                "Tocame todos los ritmos"
            ]
        ]
    },
    {
        "author": "Ibiza Pareo",
        "title": "Tunisia",
        "text": [
            [
                "Hay algo en vos",
                "Que cautiva mi ser",
                "Una conexión",
                "Más allá de vos y yo",
                "Quiero que me cuentes",
                "Todo de vos",
                "Escucharte",
                "Con el corazón"
            ],
            [
                "Quiero aprender",
                "A mirarte",
                "Sin mi mente"
            ],
            [
                "Quiero",
                "Mostrarte",
                "Quién soy"
            ],
            [
                "Te quiero",
                "Te quiero"
            ]
        ]
    },
    {
        "author": "Ibiza Pareo",
        "title": "Viajeros",
        "text": [
            [
                "Vemos realmente",
                "Esencias diferentes",
                "Tropiezan palabras",
                "Tonos subirán",
                "El agua, el aire"
            ],
            [
                "Mirá cómo nos fuimos",
                "La ira nos llevó",
                "La Luna grande, blanca",
                "Nos guiará",
                "El mundo está en caos",
                "No, no, no, no"
            ],
            [
                "Por qué miramos tanto",
                "Quién sabe qué está bien",
                "Qué es lo que estoy buscando",
                "Está en mi interior"
            ],
            [
                "Abrazaré las sombras",
                "Ver qué convive allí",
                "No quiero más pensar de más",
                "Quiero viajar"
            ]
        ]
    },
    {
        "author": "Ibiza Pareo",
        "title": "Viva, Ahí Están Los Chicos",
        "text": [
            [
                "Esta noche",
                "Voy a bailar con mis amigos"
            ],
            [
                "Celebrar",
                "Que estamos vivos"
            ],
            [
                "Disfrutar",
                "De la música"
            ],
            [
                "Música, música, música"
            ]
        ]
    }
]