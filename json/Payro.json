[
    {
        "author": "Payro",
        "title": "Aislado",
        "text": [
            [
                "Sé que estás ahí",
                "No te veo",
                "Estoy cerca de ti",
                "No te siento"
            ],
            [
                "Aislado en mi mente",
                "No sale el Sol",
                "Tú estabas ausente",
                "En un rincón"
            ],
            [
                "¿Dónde te escondes?"
            ],
            [
                "No quiero repetir",
                "El mismo error",
                "No quiero ya sentir",
                "Ese dolor"
            ]
        ]
    },
    {
        "author": "Payro",
        "title": "Despertar",
        "text": [
            [
                "Ya se borró todo el dolor",
                "Se me olvidó que siempre estuve sólo yo"
            ],
            [
                "Cambió el color se iluminó hoy",
                "El sol volvió a salir y siento su calor"
            ],
            [
                "Esto es amor"
            ],
            [
                "Lejos de dios, lejos del cielo",
                "La vida se vuelve un infierno al despertar"
            ],
            [
                "Esto es amor",
                "El sol volvió a salir y siento que es amor"
            ]
        ]
    }
]