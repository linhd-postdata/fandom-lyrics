[
    {
        "author": "Fritz Abreu",
        "title": "Dulce Poprock",
        "text": [
            [
                "Dulce poprock",
                "rojos tus cabellos",
                "labios rosados",
                "soda efervescente",
                "besos caramelo",
                "me hago adicto a tu sabor",
                "no existe un sol que me brinde tu calor"
            ],
            [
                "Nubes en el cielo",
                "palmeras mar y arena"
            ],
            [
                "Eres poprock",
                "una dulce canci"
            ]
        ]
    }
]