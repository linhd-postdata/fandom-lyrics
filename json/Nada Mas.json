[
    {
        "author": "Nada Mas",
        "title": "Los Sueños Que No Llegan",
        "text": [
            [
                "Una nube abarca",
                "Los sueños que no llegan",
                "Y la vida te garca,",
                "Metiéndote problemas que te pegan.",
                "Y vuelan, siempre llegan,",
                "Están buscando tu destino.",
                "Y se niegan, pero esperan,",
                "Están robando tu camino."
            ],
            [
                "Una sonrisa falsa",
                "y por atrás te apuñalan",
                "Querés salir de esta farsa",
                "Pero cada vez mas te señalan.",
                "Y vuelan, siempre llegan,",
                "Están buscando tu destino.",
                "Y se niegan, pero esperan,",
                "Están robando tu camino."
            ],
            [
                "Anclado a una balsa",
                "Que hundida de sueños no escapa,",
                "Intentas remar en alzas",
                "Pero este mar de dudas mas te atrapa.",
                "Y vuelan, siempre llegan,",
                "Están buscando tu destino.",
                "Y se niegan, pero esperan,",
                "Están robando tu camino."
            ]
        ]
    }
]