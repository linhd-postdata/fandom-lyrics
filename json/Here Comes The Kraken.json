[
    {
        "author": "Here Comes The Kraken",
        "title": "Ratttas",
        "text": [
            [
                "No",
                "Nos dejes caer",
                "Viviendo en la mediocridad",
                "Tu alrededor se pudre",
                "No vales nada",
                "Muerte respiras",
                "Solo traes oscuridad",
                "Arrodillate, súplica y muere",
                "Hoy llego tu final"
            ],
            [
                "Verás el fuego en tus pies",
                "Todos verán en tu cara la mierda",
                "Derramaste sangre, pobreza y dolor",
                "Destruyendo hipocresías",
                "Despiadada"
            ],
            [
                "Sodomía, escorias son, enfermos de poder",
                "Ratttas",
                "Ya no podrás joder",
                "Ratttas",
                "Naciste muerta, rata infernal"
            ],
            [
                "Lenguas de fuego te consumirán",
                "Ni tu propio dios te ayudará",
                "Tu memoria no recordarán",
                "Escorias son",
                "Escorias son"
            ],
            [
                "No hay descanso, encadenado a un",
                "Abismo infernal",
                "Abismo infernal"
            ],
            [
                "Escorias son, enfermos de poder",
                "Ratttas",
                "Ya no podrás joder",
                "Llegaste a tu fin",
                "Ratttas"
            ]
        ]
    }
]