            ],
            [
                "Que cucu, que cucu",
                "Que cucumelo."
            ],
            [
                "El cucumelero",
                "Se va pa' Misiones",
                "El cucumelero",
                "Y no quiere llover"
            ],
            [
                "El cucumelero",
                "Se ha hecho jardinero",
                "Con la regadera",
                "Riega los deshechos"
            ]
        ]
