[
    {
        "author": "Mutabor",
        "title": "Mi Vida",
        "text": [
            [
                "Mi vida es bailar",
                "Mi vida es cantar",
                "Mi vida es amor",
                "Mi vida loca",
                "Mi vida es una sonrisa",
                "Es un beso",
                "Mi vida qué sabrosa",
                "Mi vida asi me gusta"
            ],
            [
                "Mi vida es un grande concierto",
                "Una canción que siempre se repite",
                "La música mi vida",
                "Como la sal en la comida"
            ],
            [
                "Mi vida es dolor",
                "Mi vida es miedo",
                "Mi vida es hambre",
                "No sé qué pasa",
                "Mi vida es trabajo",
                "Está muerta",
                "Mi vida pasada",
                "Asi no me gusta"
            ]
        ]
    }
]