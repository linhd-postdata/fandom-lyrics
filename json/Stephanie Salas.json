[
    {
        "author": "Stephanie Salas",
        "title": "Ave María",
        "text": [
            [
                "Que fácil se nos hizo",
                "Amarnos a escondidas",
                "Comprar un paraíso",
                "En el último piso",
                "Frente a donde vivías"
            ],
            [
                "Un amor sin permiso",
                "Dos llamas encendidas",
                "Y nació un compromiso",
                "En el último piso",
                "Cuando aún me quería"
            ],
            [
                "Coro:",
                "Ave María",
                "Hoy estoy sola",
                "Con mi conciencia"
            ],
            [
                "Ave María",
                "Con mi pecado",
                "Y mi penitencia"
            ],
            [
                "Te fuiste sin aviso",
                "Mi jinete sin bridas",
                "Y yo me martirizo",
                "Cuando el ayer reviso",
                "Y solo encuentro... heridas"
            ],
            [
                "Tu amor se me deshizo",
                "Mis manos vencidas",
                "Un amor enfermizo",
                "En el último piso",
                "Separó nuestras vidas"
            ],
            [
                "Coro...",
                "Ave María",
                "Dios me perdone",
                "Si aún te quiero"
            ],
            [
                "Ave María",
                "Quiero del cielo",
                "Ser un lucero"
            ],
            [
                "Ave María",
                "Sé que sus cuerpos",
                "Se juntan de nuevo"
            ],
            [
                "Ave María",
                "Lánzarme al vacío",
                "Señor, Yo no me atrevo"
            ],
            [
                "Ave María",
                "Por su abandono",
                "Ni lloro, ni me quejo"
            ],
            [
                "Ave María",
                "Y en mi sendero",
                "A los dos les dejo"
            ],
            [
                "Ave María",
                "Se repite primer coro."
            ]
        ]
    }
]