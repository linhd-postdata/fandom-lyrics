[
    {
        "author": "Adolfo Urias",
        "title": "Corazon Chiquito",
        "text": [
            [
                "Corazon chiquititito",
                "linda flor de clavelito",
                "ahi quien pudiera tenerte",
                "para darte de besitos"
            ],
            [
                "esos labios que sostienen",
                "esa flor tan encarnada",
                "es la causa",
                "que mi alma de ti esta enamorada"
            ],
            [
                "por esos lindos ojos",
                "te doy toda mi vida",
                "por esos lindos labios",
                "te doy todo mi amor"
            ],
            [
                "estoy en cuerpo y alma",
                "rendido hasta tus plantas",
                "brindandote carino",
                "con toda mi pasion"
            ],
