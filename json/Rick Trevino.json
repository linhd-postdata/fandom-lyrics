[
    {
        "author": "Rick Trevino",
        "title": "Un Momento Alla",
        "text": [
            [
                "Que bonito fue,",
                "fuimos un par",
                "y llenos de amor,",
                "un momento alla"
            ],
            [
                "No existia el bien",
                "ni tampoco el mal,",
                "nada mas tu y yo",
                "y un momento alla"
            ],
            [
                "Refran"
            ],
            [
                "Dias sin fin,",
                "noches tambien,",
                "amor asi, nomas uno en",
                "la vida",
                "Entregare",
                "todo me ser",
                "por un momento alla"
            ],
            [
                "Te siento temblar,",
                "Es porque te vas?",
                "O piensas quedarte",
                "un momento mas?"
            ],
            [
                "La luz es amor,",
                "no se acabara",
                "Logramos brillar",
                "un momento alla"
            ],
            [
                "Dias sin fin,",
                "noches tambien,",
                "amor asi, nomas uno en",
                "la vida",
                "Entregare",
                "todo me ser",
                "por un momento alla"
            ],
            [
                "por un momento alla"
            ]
        ]
    }
]