[
    {
        "author": "Raimundo Amador",
        "title": "Bolleré",
        "text": [
            [
                "Ya no puedo olvidarte",
                "Ya no puedo olvidarte ya",
                "Ya no puedo olvidarte",
                "Ahora, ahora."
            ],
            [
                "Ya no puedo dejarte",
                "Ya no puedo dejarte ya",
                "Ya no puedo dejarte",
                "Ahora Ahora."
            ],
            [
                "Bolleré les quita las penas",
                "Bolleré les vuelve locos",
                "Bolleré pa toda la vida",
                "Bolleré que papel",
                "Bolleré Bolleré Bolleré Bolleré",
                "Bolleré Bolleré Bolleré qué papel."
            ],
            [
                "Ya no puedo besarte",
                "Ya no puedo besarte ya",
                "Ya no puedo besarte",
                "Ahora ahora."
            ]
        ]
    }
]