[
    {
        "author": "Juan Carlos Flores",
        "title": "Corazón De Niña",
        "text": [
            [
                "Cada vez",
                "que mis pies",
                "dan un paso",
                "pienso en ti",
                "algo natural",
                "tanto como respirar",
                "porque se",
                "que me ves",
                "aunque me quiera esconder",
                "hasta cuando duermo",
                "te siento en la piel"
            ],
            [
                "corazón de niña",
                "sueños de mujer",
                "basta una sonrisa",
                "para que me siente bien",
                "corazon de niña",
                "alma de mujer",
                "sueño cada dia",
                "con tu voz de miel"
            ],
            [
                "al calor",
                "del amor",
                "hasta en plena noche hay sol",
                "eso es lo mejor",
                "que nos podia ocurrir"
            ],
            [
                "cuando dos",
                "como tu",
                "y yo sentimos asi",
                "damos gracias a dios",
                "que nos puso aqui"
            ],
            [
                "Corazón de niña",
                "sueños de mujer",
                "basta una sonrisa",
                "para que me siente bien",
                "corazon de niña",
                "alma de mujer",
                "sueño cada dia",
                "con tu voz de miel"
            ],
            [
                "Corazón de niña",
                "sueños de mujer",
                "basta una sonrisa",
                "para que me siente bien",
                "corazon de niña",
                "alma de mujer",
                "sueño cada dia",
                "con tu voz de miel"
            ],
            [
                "Corazón de niña",
                "sueños de mujer",
                "basta una sonrisa",
                "para que me siente bien",
                "corazon de niña",
                "alma de mujer",
                "sueño cada dia",
                "con tu voz de miel"
            ]
        ]
    }
]