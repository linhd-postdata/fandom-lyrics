[
    {
        "author": "La Batalla",
        "title": "Cuando Estas Conmigo",
        "text": [
            [
                "Cuando estas conmigo",
                "Se llena mi corazon.",
                "Tiene sentido la vida",
                "Y el mundo es una ilucion.",
                "Cuando estas conmigo",
                "La noche tiene color.",
                "Un beso es suficiente",
                "Para entender el amor."
            ],
            [
                "Y al abrasarte",
                "Y sentir que me amas",
                "Salen las estrellas",
                "Aun si es de dia.",
                "Y todos esos dias",
                "Llenos de vacio",
                "Se bolvieron polvo",
                "Por que estas conmigo."
            ],
            [
                "Cuando estas conmigo",
                "Es bello mi alrededor.",
                "Una sonrisa tuya",
                "Derrite mi corazon.",
                "Cuando estas conmigo",
                "El tiempo tiene valor.",
                "Una carizia tuya",
                "Me hace bolar hasta el sol."
            ],
            [
                "(x3)",
                "Y al abrasarte",
                "Y sentir que me amas",
                "Salen las estrellas",
                "Aun si es de dia.",
                "Y todos esos dias",
                "Llenos de vacio",
                "Se bolvieron polvo",
                "Por que estas conmigo."
            ]
        ]
    }
]