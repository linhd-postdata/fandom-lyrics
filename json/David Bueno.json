[
    {
        "author": "David Bueno",
        "title": "Tu Sueño Hasta El Final",
        "text": [
            [
                "Habla de tí",
                "Habla de mí",
                "De confiar",
                "Y de soñar",
                "Habla de amigos y un destino",
                "Que piensan exigir"
            ],
            [
                "Habla del cielo conquistar,",
                "(¡Pokémon!)",
                "Con gran coraje,",
                "Sin miedo a luchar"
            ],
            [
                "Habla de no rendirse y ser",
                "Valiente y proteger",
                "Tu sueño hasta el final",
                "(¡Y vencer en la Liga Sinnoh!)",
                "¡Pokémon!"
            ]
        ]
    }
]