[
    {
        "author": "Auricular",
        "title": "Imperfecto",
        "text": [
            [
                "No me vengás a decir",
                "Que es invierno",
                "En enero"
            ],
            [
                "No me digás de correr",
                "Si no hay nada",
                "Que ofrecer"
            ],
            [
                "Hay un libro en un libro, en un libro, en un libro..."
            ],
            [
                "Que dentro",
                "De poco",
                "Vas a ver"
            ],
            [
                "Un satélite enlazó",
                "Un espejo",
                "Un reflejo"
            ],
            [
                "Una sola imperfección",
                "Que es de todas",
                "La peor"
            ],
            [
                "Hay un libro en un libro, en un libro, en un libro..."
            ],
            [
                "Que dentro",
                "De poco",
                "Vas a ver"
            ],
            [
                "Dentro",
                "De poco",
                "Vas a ver"
            ]
        ]
    },
    {
        "author": "Auricular",
        "title": "Incierto",
        "text": [
            [
                "Sos verdad en",
                "El incierto",
                "Sos algo que yo...",
                "Puedo amar"
            ],
            [
                "Dentro de",
                "Una canción",
                "Te voy a pedir...",
                "Por favor"
            ],
            [
                "Hoy quisiste",
                "Descubrirme",
                "Miradas en mí...",
                "Voy a ver"
            ],
            [
                "Quiero que",
                "No cuentes nada",
                "Una almohada para dormir",
                "Bien la siesta"
            ],
            [
                "Quiero que veas en mí",
                "La noche que pasó"
            ]
        ]
    },
    {
        "author": "Auricular",
        "title": "Lluvia Por Debajo Del Agua",
        "text": [
            [
                "Por puntos sostenidos",
                "Detenidos por luz"
            ],
            [
                "Gotas de lluvia veo caer",
                "Baile entre la oscuridad",
                "Y luz"
            ],
            [
                "Claroscuro un murmullo es",
                "Susurrarán",
                "Claroscuro un murmullo es",
                "Susurrarán"
            ],
            [
                "Debajo del agua, agua va",
                "Desde mi ventana"
            ],
            [
                "Lento llanto fue alguna vez",
                "Gotas piensan caer y bailar tal vez"
            ]
        ]
    },
    {
        "author": "Auricular",
        "title": "Salto",
        "text": [
            [
                "Salto",
                "Desde el cielo"
            ],
            [
                "Vuelo",
                "No veo el suelo"
            ],
            [
                "Caigo",
                "En lento vaivén"
            ],
            [
                "Veo el cielo",
                "Donde vuelo",
                "Vuelo y doblego el aire",
                "Caigo y veo el paisaje"
            ],
            [
                "Se aproxima",
                "Como en un Sur"
            ]
        ]
    },
    {
        "author": "Auricular",
        "title": "Un Color",
        "text": [
            [
                "Ya me",
                "Queda",
                "Poco",
                "Tiempo"
            ],
            [
                "No te",
                "Puedo",
                "Decir quién soy"
            ],
            [
                "Un estímulo, un secreto",
                "Un color"
            ],
            [
                "El tiempo será",
                "De un color",
                "Que iluminará"
            ],
            [
                "Ya te",
                "Queda",
                "Poco",
                "Tiempo"
            ],
            [
                "No te",
                "Puede",
                "Decir quién es"
            ],
            [
                "Su estímulo secreto",
                "Su color"
            ],
            [
                "Y el tiempo será",
                "De un color tan claro",
                "Que iluminará"
            ],
            [
                "Un estímulo, un estímulo...",
                "Un secreto",
                "Un color"
            ]
        ]
    }
]