[
    {
        "author": "Club Musical Oriente Cubano",
        "title": "Sabanimar",
        "text": [
            [
                "Defiende bien tu conuco",
                "Defiende bien tu pinar",
                "Defiende bien lo que es tuyo",
                "Hombre de Sabanimar"
            ],
            [
                "(Coro)",
                "ﾃソ"yelo bien, guajiro",
                "ﾃソ"yelo bien, Sabanimar"
            ],
            [
                "No permitas que te roben",
                "El lugar de tu nacer",
                "Tus redes y tus aperos",
                "Tu caballo y tu mujer"
            ],
            [
                "Que no vengas los bandidos",
                "Ladrones de tierra y mar",
                "A llevarse lo que es tuyo"
            ],
            [
                "Tu hermoso Sabanimar"
            ],
            [
                "El hombre de pelo en pecho",
                "Que se sabe defender",
                "Defiende con tierra y bello",
                "Su caballo y su mujer"
            ],
            [
                "Con la pala o con el pico",
                "Con dientes, piedras o palos",
                "Defiende por bien nacido",
                "Lo que te quieren robar",
                "ﾃソ"yelo bien, guajiro",
                "ﾃソ"yelo bien, Sabanimar"
            ]
        ]
    }
]