[
    {
        "author": "Meat Loaf",
        "title": "Monstro",
        "text": [
            [
                "Tu eres cruel (monstro)",
                "No eres fiel (monstro)",
                "Hijo del mal (monstro)",
                "Un ser fatal (monstro)",
                "Tu corazon (monstro)",
                "Es un ladron (monstro)"
            ]
        ]
    }
]