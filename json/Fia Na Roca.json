[
    {
        "author": "Fia Na Roca",
        "title": "Fia Na Roca",
        "text": [
            [
                "Críate e vaite criando",
                "Rosa branca do xardín",
                "Críate, vaite criando",
                "Que te crías para min"
            ],
            [
                "Fía, fía, fía na roca",
                "Fía, fía, fai mazaroca",
                "Fía, fía, fía na roca",
                "Ai la la la la la la la"
            ],
            [
                "A moza que é caladiña",
                "E non di mal de ninguenhe",
                "Canto máis baixiño mira",
                "Cantos mais amores ten"
            ],
            [
                "Fía, fía, fía na roca",
                "Fía, fía, fai mazaroca",
                "Fía, fía, fía na roca"
            ]
        ]
    }
]