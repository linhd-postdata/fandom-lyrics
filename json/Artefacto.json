[
    {
        "author": "Artefacto",
        "title": "Llevame",
        "text": [
            [
                "Si tal vez",
                "viajara para tu ser",
                "sentir la paz",
                "que tiene tu mirar"
            ],
            [
                "si tal vez",
                "pudieras entender",
                "soñar, vivir contigo de verdad..."
            ],
            [
                "llévame donde nunca fui",
                "no te voy a detener",
                "libérame de la realidad",
                "quiero verte otra vez"
            ],
            [
                "llévame...",
                "libérame..."
            ]
        ]
    }
]