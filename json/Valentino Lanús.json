[
    {
        "author": "Valentino Lanús",
        "title": "Por Ti",
        "text": [
            [
                "Por ti lo que daria",
                "me juego hasta la vida",
                "por ti dejo el futuro",
                "te sigo al fin del mundo",
                "por una mirada",
                "por una esperanza",
                "daria todos mis sueños",
                "por sentir tan solo un beso"
            ],
            [
                "si tu fueras sirena en algun puerto",
                "por ti me volveria marinero",
                "si solo te fijaras un momento",
                "verias en mis ojos lo que siento"
            ],
            [
                "Si fueras la princesa encadenada",
                "por ti yo ganaria mil batallas",
                "amor mas transparente",
                "y verdadero",
                "jamas encontraras",
                "a donde vayas."
            ]
        ]
    }
]