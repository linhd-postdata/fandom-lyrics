[
    {
        "author": "Adriana",
        "title": "Lindo Cielo",
        "text": [
            [
                "Por sobre las nubes",
                "Existi um um lindo cielo",
                "Maravilloso cielo",
                "Morada de ángeles"
            ],
            [
                "Por sobre las nubes",
                "Existe un trono",
                "Donde el Rey esta sentado",
                "A la derecha de Dios"
            ],
            [
                "Cielo, lindo cielo",
                "Es el lugar",
                "Donde quiero vivir",
                "Por siempre"
            ],
            [
                "Cielo, lindo cielo",
                "Es el lugar",
                "Que Dios preparó",
                "Para mi"
            ],
            [
                "Cielo, lindo cielo",
                "Con los ángeles",
                "Yo cantaré",
                "Adorando al Señor"
            ]
        ]
    }
]