[
    {
        "author": "Chubby Checker",
        "title": "La Bamba",
        "text": [
            [
                "Para bailar la bamba",
                "Para bailar la bamba",
                "Se necessita una poca de gracia"
            ],
            [
                "Una poca de gracia, para mi, para ti",
                "Ay arriba arriba, ay arriba, arriba",
                "Por ti sere, por ti sere, por ti sere"
            ],
            [
                "Yo no soy marinero",
                "Yo no soy marinero",
                "Soy capitan",
                "Soy capitan",
                "Soy capitan"
            ],
            [
                "Bamba, bamba bamba"
            ]
        ]
    }
]