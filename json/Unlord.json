[
    {
        "author": "Unlord",
        "title": "E Caha Di Bela",
        "text": [
            [
                "anochi seur, diablo ta lur",
                "muhernan si dono",
                "ta perde non cono",
                "mesito",
                "miedoso ru bosnan ta",
                "adorande i alabands",
                "otro diosnan",
                "dios no lo tolera",
                "alma",
                "ainda de caha di bela",
                "ta banda di e misa",
                "o, dios pordonanos",
                "pa nos pecadornan",
                "sirbidor di satanas"
            ],
            [
                "scupiendo awa bendita",
                "rudianan ta sangria"
            ],
            [
                "blasphemia total"
            ]
        ]
    }
]