[
    {
        "author": "Capital Inicial",
        "title": "De Musica Ligeira",
        "text": [
            [
                "Ella durmió",
                "Al calor de las masas",
                "Y yo desperté",
                "Queriendo soñaria",
                "Algún tiempo atrás",
                "Pensé en escribirie",
                "Que nunca sorteé",
                "Las trampas del amor"
            ],
            [
                "De aquel amor",
                "De música ligeira",
                "Nada nos libra",
                "Nada mas queda"
            ],
            [
                "No le enviaré",
                "Cenizas de rosas",
                "Ni pienso evitar",
                "Un roce secreto"
            ],
            [
                "De aquel amor",
                "De música ligeira",
                "Nada nos libra",
                "Nada mas queda"
            ],
            [
                "Nada mas queda",
                "Nada mas queda",
                "Nada mas queda"
            ]
        ]
    }
]