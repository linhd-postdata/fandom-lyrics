[
    {
        "author": "Eddie Espinosa",
        "title": "Es Tu sangre",
        "text": [
            [
                "Es Tu sangre que redime",
                "Tu sangre de la vida me da Es Tu sangre carmesí",
                "Que tomó mi lugar",
                "Blanco como la nieve",
                "pura soy (ya lo soy)",
                "Mi Cristo, Cordero de Dios"
            ]
        ]
    }
]