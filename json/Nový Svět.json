[
    {
        "author": "Nový Svět",
        "title": "Sin Palabras",
        "text": [
            [
                "A kilómetros estás tan inalcanzable",
                "Y no sabemos ni nuestros nombres",
                "Tu sola presencia es lo que deseo",
                "Extraño captar sin palabras"
            ],
            [
                "Cada día pregunta su noche",
                "Cada día pregunta su fín",
                "Cada amor pasa pronto",
                "Y tu estás tan lejos de mí"
            ],
            [
                "Cada día pregunta su noche",
                "Cada vida pregunta su fín",
                "Y lo no puedo evitar",
                "No puedo..."
            ],
            [
                "Entender y escuchar sin palabras",
                "Extraño captar sin palabras"
            ],
            [
                "Y cada día pregunta su noche",
                "Cada vida pregunta su fín",
                "Cada amor pasa pronto",
                "Y lo no puedo evitar"
            ],
            [
                "En los infernos me esperan",
                "En los infernos a mí me esperan"
            ],
            [
                "Me esperan..."
            ]
        ]
    }
]