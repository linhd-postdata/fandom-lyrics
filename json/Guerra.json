[
    {
        "author": "Guerra",
        "title": "Chiapas",
        "text": [
            [
                "? y miren lo que son las cosas",
                "Porque para que nos vieran",
                "Nos tapamos el rostro",
                "Para que nos nombraran",
                "Nos negamos el nombre",
                "Apostamos el presente"
            ],
            [
                "Para tener futuro",
                "Y para vivir",
                "Morimos?"
            ]
        ]
    }
]