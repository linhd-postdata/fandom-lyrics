[
    {
        "author": "Julieta Venegas & Ana Tijoux",
        "title": "Lo Que Tu Me Das",
        "text": [
            [
                "Solo te quiero pedir",
                "Que siempre me mires así",
                "Como lo haces ahora",
                "Me has hecho despertar"
            ],
            [
                "La vida nos ha hecho ya",
                "Y en todo momento cambiará",
                "Luego me miras así",
                "Mi fe se levanta",
                "Soy otra yo"
            ],
            [
                "Y sé",
                "Que todo puede cambiar",
                "Pero siempre viviré",
                "Con lo que",
                "Tu me das"
            ],
            [
                "Solo te quiero decir",
                "Que siempre me mires así",
                "Como lo haces ahora",
                "Me has hecho despertar"
            ],
            [
                "Si un día estoy lejos de aquí",
                "Recuerda lo que fui",
                "Yo seguiré",
                "Cerca de ti lo sabes",
                "El viento",
                "Te hará olvidar"
            ],
            [
                "Y sé",
                "Que todo puede cambiar",
                "Pero siempre viviré",
                "Con lo que",
                "Tu me das"
            ]
        ]
    }
]