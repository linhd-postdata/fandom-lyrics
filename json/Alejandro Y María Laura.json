            [
                "oye olvídala, olvídala",
                "que si no te matará",
                "olvídala"
            ],
            [
                "Déjala, recuérdala…"
            ],
            [
                "hombre cuídala, olvídala",
                "cuídala, olvídala…"
            ]
        ]
