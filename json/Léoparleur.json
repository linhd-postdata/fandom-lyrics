[
    {
        "author": "Léoparleur",
        "title": "No Dice Nada",
        "text": [
            [
                "Bajo el fuego de tus ojos",
                "Bajo el cariño de tus manos",
                "No dice nada",
                "No dice nada",
                "No dice nada"
            ],
            [
                "Bajo el frio del invierno",
                "La lluvia, la nieve y el viento",
                "No dice na"
            ],
            [
                "Bajo terror, guerra, infierno",
                "Todas esas cosas, terramoto",
                "No dice nada",
                "No dice nada",
                "No dice nada"
            ],
            [
                "Bajo el hielo que la rompe",
                "La mano que la tira lejos",
                "La mano que la recoje",
                "Y el cariño de tus delos",
                "Bajo el fuego de tus ojos"
            ],
            [
                "La piedra nunca dice nada",
                "La piedra nunca dice",
                "La piedra nunca dice nada",
                "La piedra nunca dice nada",
                "Nunca dice nada"
            ],
            [
                "No dice nada",
                "No dice nada",
                "No dice nada"
            ],
            [
                "Bajo el hielo que la rompe",
                "La mano que la tira lejos",
                "La mano que la recoje",
                "Y el cariño de tus delos",
                "Bajo el fuego de tus ojos"
            ]
        ]
    }
]