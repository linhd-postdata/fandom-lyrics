[
    {
        "author": "Joann Rosario",
        "title": "Cuando Reposo En Ti",
        "text": [
            [
                "Verse",
                "Temprano en la manana",
                "Tu rostro buscare",
                "Encuentro mi vida",
                "Y gozo sin igual",
                "Toda ansiedad y duda",
                "Se va di mi",
                "Cuando reposo en Ti",
                "Cuando reposo en Ti"
            ],
            [
                "Chorus",
                "Encuentro mi vida en Ti",
                "Cuando reposo en Ti",
                "Mi paz es completa",
                "Cuando reposo en Ti",
                "Mi fe es renovada",
                "Cuando reposo en Ti",
                "Estoy rodeado",
                "Por el poder de Tu amor"
            ]
        ]
    }
]