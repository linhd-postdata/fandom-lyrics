[
    {
        "author": "Roxana Puente",
        "title": "Déjame Ser",
        "text": [
            [
                "Hoy me despierto",
                "En una nube de ansiedad",
                "Extraño tus besos",
                "No puedo mas",
                "Paso los dias",
                "Sin entender que sucedio",
                "Por que no estas aqui",
                "Por que me fui"
            ],
            [
                "Sigo intentando aceptar",
                "Que lo nuestro llego a su final",
                "Y que todo acabo pero hoy me arrepiento..."
            ],
            [
                "CORO:",
                "Dejame ser",
                "Por una vez",
                "Solo tu y yo amor",
                "Dame el corazon",
                "No me dejes sin razon",
                "Mirame bien",
                "Aunque sea mienteme esta vez",
                "Que fui muriendo",
                "Ya no entiendo",
                "Separarnos fue mi error"
            ],
            [
                "Un dia mas",
                "Sueño que estas entre mis sabanas",
                "Abrazandome...",
                "Protegiendome"
            ],
            [
                "Quiero pensar que esto no es el final",
                "Y dejar de llorar por que tu ya no estas",
                "Simplemente no puedo",
                "No quiero..."
            ],
            [
                "Dejame ser",
                "Por una vez",
                "Solo tu y yo amor",
                "Dame el corazon",
                "No me dejes sin razon",
                "Mirame bien",
                "Aunque sea mienteme esta vez",
                "Que fui muriendo",
                "Ya no entiendo",
                "Separarnos fue mi error"
            ],
            [
                "Fue mi error...dejame ser",
                "Por una vez",
                "Solo tu y yo amor",
                "Dame el corazon",
                "No me dejes sin razon",
                "Mirame bien",
                "Aunque sea mienteme esta vez",
                "Que fui muriendo",
                "Ya no entiendo",
                "Separarnos fue mi error"
            ],
            [
                "Hoy desperte",
                "Mis lagrimas han sasado ya",
                "Vivire en orden"
            ]
        ]
    }
]