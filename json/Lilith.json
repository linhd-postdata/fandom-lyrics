[
    {
        "author": "Lilith",
        "title": "Muñecas",
        "text": [
            [
                "Fui al cielo de las muñecas",
                "Es donde duermen",
                "Las niñas muertas",
                "Chananana, chananana, chananana"
            ],
            [
                "Puedes comerte sus piruletas",
                "Gotas espesas",
                "Y alguna seta",
                "Chananana, chananana, chananana"
            ],
            [
                "Oh no, meu"
            ],
            [
                "Nadie quiere a las niñas muertas",
                "Ni las recuerdan."
            ],
            [
                "En los ojos tengo violetas",
                "Y en los brazos",
                "Unas cadenas",
                "Chananana, chananana, chananana"
            ],
            [
                "En el cielo de las muñecas",
                "Las lágrimas saben",
                "A moras y fresas",
                "Chananana, chananana, chananana"
            ],
            [
                "Oh no, meu"
            ],
            [
                "Nadie quiere a las niñas muertas",
                "Ni las recuerdan."
            ]
        ]
    }
]