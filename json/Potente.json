[
    {
        "author": "Potente",
        "title": "Linda Morenita",
        "text": [
            [
                "Linda Morenita",
                "De ojitos cafés",
                "Yo te quiero mucho",
                "Ya volví otra vez",
                "Sabes que te quiero",
                "Con todo el corazón",
                "Nunca se me olvida",
                "El amor de los dos"
            ],
            [
                "Y ahora",
                "Juntos los dos",
                "Viviremos felices",
                "Bien sabes mi amor",
                "Nunca morenita",
                "Me olvido de ti",
                "Mi amor y mi vida",
                "Solo son para ti"
            ],
            [
