[
    {
        "author": "Polanko",
        "title": "Vamos A Ver",
        "text": [
            [
                "VAMOS A VER"
            ],
            [
                "Tienes esa mirada",
                "Que me provoca acercarme a ti",
                "Y querer descubrir que no es tan difícil",
                "Encontrar algo dentro de ti"
            ],
            [
                "Quédate cerca",
                "Mírame nena",
                "Vamos a ver lo que escondes tan bien",
                "Debajo de la piel"
            ],
            [
                "Vamos a ver lo que hay dentro",
                "Al fondo del anochecer",
                "Vamos a ver lo que encuentro",
                "Debajo de lo que llevas puesto"
            ],
            [
                "Tienes esa manera",
                "De hacer que me pierda en ti",
                "Sin tener que sentir las caricias que un día",
                "Se encontraron lejos de ti"
            ],
            [
                "Quédate cerca",
                "Mírame nena"
            ],
            [
                "Vamos a ver lo que hay dentro",
                "Al fondo del anochecer",
                "Vamos a ver lo que encuentro",
                "Debajo de lo que llevas puesto"
            ],
            [
                "Vamos a ver",
                "Lo que llevas dentro",
                "Vamos a ver",
                "Lo que llevas dentro"
            ],
            [
                "Vamos a ver lo que hay dentro",
                "Al fondo del anochecer",
                "Vamos a ver lo que encuentro"
            ]
        ]
    }
]