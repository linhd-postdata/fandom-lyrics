[
    {
        "author": "Gustavo Munguía & Lorena De La Garza",
        "title": "La Hora Pico",
        "text": [
            [
                "Nuestro mundo en la actualidad",
                "Es muy difícil y duro de pelar",
                "Pero major hágame un gran favor",
                "Y tómelo con buen humor"
            ],
            [
                "La hora pico",
                "Siéntese por favor",
                "La hora pico",
                "Acomódese en el sillón",
                "La hora pico",
                "Le va a gustar",
                "La hora pico",
                "Ya va a empezar"
            ],
            [
                "La hora pico",
                "Le pone sabor",
                "La hora pico",
                "Aflójese en el sillón",
                "La hora pico",
                "Se me va de bolón",
                "La hora pico",
                "Que buen reventón"
            ]
        ]
    }
]