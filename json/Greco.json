[
    {
        "author": "Greco",
        "title": "Ahora O Siempre",
        "text": [
            [
                "El invierno se hizo para recordar",
                "Que va a volver la primavera",
                "Alguna vez tuvimos muchas Navidades",
                "El mundo era más grande que el rincón que era"
            ],
            [
                "Ahora o siempre",
                "Que si ahora o siempre",
                "Ahora o siempre",
                "Siempre cerca"
            ],
            [
                "Se cuela el frío inmaculado",
                "Los esqueletos que hay en las postales",
                "Fue a buscar",
                "Los árboles",
                "Y sobre el mar el beso",
                "Te ahogue"
            ],
            [
                "Ahora o siempre",
                "Que si ahora o siempre",
                "Ahora o siempre",
                "Siempre cerca"
            ]
        ]
    }
]