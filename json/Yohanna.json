[
    {
        "author": "Yohanna",
        "title": "Si te vas",
        "text": [
            [
                "Me dices que me amas",
                "A veces sí hay llamas",
                "Que se veen caer",
                "Hablan de mentiras",
                "¿Sólo son mentiras?",
                "Tengo que saber"
            ],
            [
                "¿Se termina el sueño ya?",
                "Al bajar del azúl"
            ],
            [
                "Si te vas (Si te vas)",
                "¿Se acaba?",
                "¿Puedo vivir sin tí?",
                "¿Fuiste tu? (¿Fuiste tu?)",
                "Me dijiste que no te irías así"
            ],
            [
                "No sabes entenderme",
                "¿No basta ya quererte?",
                "El amor se va",
                "Con estas decepciones",
                "Tormentas y pasiones",
                "¿Se terminará?"
            ],
            [
                "Se termina el sueño ya",
                "Al bajar del azúl"
            ],
            [
                "Si te vas (Si te vas)",
                "¿Se acaba?",
                "¿Puedo vivir sin tí",
                "¿Fuiste tu? (¿Fuiste tu?)",
                "Me dijiste que no te irías así"
            ],
            [
                "Si te vas (Si te vas)",
                "¿Se acaba?",
                "¿Puedo vivir sin tí?",
                "¿Fuiste tu? (¿Fuiste tu?)",
                "Me dijiste que no te irías así"
            ],
            [
                "(Si te vas)",
                "(¿Se acaba?)",
                "¿Se irá mi dolor?",
                "¿Si te vas? (¿Si te vas?)",
                "¿Se acaba?",
                "¿Yo abandonada así?"
            ],
            [
                "Si te vas"
            ]
        ]
    }
]