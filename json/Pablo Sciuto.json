[
    {
        "author": "Pablo Sciuto",
        "title": "7 Am",
        "text": [
            [
                "Silencio, quietud,",
                "Fundido azul,",
                "Ventana, hogar,",
                "Macetas sin regar.",
                "Ropa esperando secar",
                "Luz tenue que no quiere madrugar."
            ],
            [
                "Se enciende, la ciudad",
                "Las calles sin pisar"
            ],
            [
                "Y yo desde otro lugar",
                "Esperando verte despertar."
            ],
            [
                "Coro de despertador,",
                "Gritos desde otro sal"
            ]
        ]
    }
]