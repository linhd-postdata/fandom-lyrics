[
    {
        "author": "Ataraxia",
        "title": "Lucrecia",
        "text": [
            [
                "A los largos entierros",
                "Sin tambores ni la musica",
                "Desfilan lentamente",
                "En mi alma",
                "En mi alma"
            ],
            [
                "(Charles Baudelaire)"
            ]
        ]
    }
]