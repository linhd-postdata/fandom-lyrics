[
    {
        "author": "Lampara",
        "title": "Elevador",
        "text": [
            [
                "La vida es una nota",
                "Que da vueltas",
                "Entre mi corazon",
                "Quiero conservar",
                "Todas las mentiras",
                "Para luego tirarlas",
                "En lo mas profundo",
                "De la verdad"
            ]
        ]
    }
]