        "author": "Gigi D'Agostino",
        "title": "Cada Vez",
        "text": [
            [
                "Cada vez que te veo..."
            ],
            [
                "El escritor en una mesa",
                "Colocada en la vereda",
                "Se juntaban los amigos",
                "A discutir la situación"
            ],
            [
                "Cade vez que te veo",
                "Me acuerdo de esos tiempos",
                "Con las cervezas a litros",
                "Mirando la puesta del sol"
            ],
            [
                "El escritor en una mesa",
                "Colocada en la vereda",
                "Se juntaban los amigos",
                "A discutir la situación"
            ],
            [
                "Cada vez que te veo",
                "Cada vez que te veo",
                "Cada vez que te veo",
                "Cada vez que te veo",
                "Cada vez...",
                "Cada vez que te veo"
            ],
            [
                "Cada vez que te veo",
                "La noche se transforma",
                "En una serie de recuerdos",
                "Casi pura ilusión"
            ],
            [
                "Repitiendo los mismos cuentos",
                "De vez en cuando siento",
                "La brisa de esa playa",
                "Flamiyando mi pantalón"
            ],
            [
                "Cada vez que te veo",
                "Cada vez que te veo",
                "Cada vez que te veo",
                "Cada vez que te veo",
                "Cada vez...",
                "Cada vez que te veo"
            ],
            [
                "Why me...",
                "Why me...",
                "Why me causa placer al oír",
                "Esa brisa que tanto sentí",
                "Why saber",
                "Que por ese momento",
                "Nada cambió"
            ],
            [
                "Why me trae tantos recuerdos",
                "De una época que yo viví",
                "Why ahí pensás quedarte",
                "Pero yo no estoy ahí"
            ],
            [
                "Cada vez que te veo",
                "Cada vez que te veo",
                "Cada vez que te veo",
                "Cada vez que te veo",
                "Cada vez..."
            ]
        ]
    }
