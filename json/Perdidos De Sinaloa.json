[
    {
        "author": "Perdidos De Sinaloa",
        "title": "Te Vas",
        "text": [
            [
                "Se que me has engañado",
                "No puedo perdonarte lo sabes bien",
                "Tú me fallaste"
            ],
            [
                "De mí tu te burlaste",
                "Y ahora quieres irte",
                "Que te vaya bien..."
            ],
            [
                "Te vas",
                "Tal vez has de pensar que te voy a detener"
            ],
            [
                "No sé",
                "No sé ni como diablos de ti me enamoré"
            ],
            [
                "Al fin",
                "De tus mentiras y tus engaños yo me libre"
            ],
            [
                "Y buscaré en otro amor",
                "Que me quiera como soy"
            ],
            [
                "Y encontraré la felicidad",
                "Que tu amor nunca me dió"
            ]
        ]
    }
]