            [
                "No hay para mí un lugar,",
                "Ni por mí lo habrá,",
                "Que me calme.",
                "Ni hay hornos de cocción",
                "Para el corazón,",
                "Que desarmen",
                "Lo que sé,",
                "Porque viene y se va."
            ],
            [
                "Y aunque lo asuma,",
                "No pierde amargura."
            ],
            [
