[
    {
        "author": "Camera Obscura",
        "title": "Amigo Mío",
        "text": [
            [
                "Permanece detrás",
                "No tan cerca de mí",
                "No sabes dónde he estado"
            ],
            [
                "No te sientes a mi lado",
                "Permanece tan lejos de mí",
                "Como yo quiero estar de ti"
            ],
            [
                "Este sitio es tan frío",
                "No hables por mí",
                "No quiero hablar",
                "Es perder mi tiempo",
                "No me hagas reír",
                "No me hagas llorar",
                "No me preguntes mi nombre",
                "No me mires a los ojos",
                "Amigo mío"
            ]
        ]
    }
]