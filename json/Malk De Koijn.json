[
    {
        "author": "Malk De Koijn",
        "title": "Los Salvajas (Caracoles Marinos)",
        "text": [
            [
                "Questo capello el moderno"
            ],
            [
                "A jerra",
                "A semi gusto que compadre",
                "Pollo loco cigarillos",
                "Fanto lasso semi Pablo",
                "El Prego",
                "Con si la saños de la pek pek",
                "A zorro fronto Kimi Larso",
                "Los muchachas q-que paso"
            ],
            [
                "A Jerra",
                "A semi gustu que compadre \"e soy compadros\"",
                "Collo loco cigarillos \"fueme los cigarrilos\"",
                "Fanto lasso semi Pablo",
                "El Prego \"con si lagurnas\"",
                "Con si la saños de la pek pek",
                "\"Prado!\"",
                "A Zorro fronto Kimi Larso \"no fronto mi mano\"",
                "Los muchachas q-que paso",
                "\"El Prego mal dano\""
            ],
            [
                "\"Alora\"",
                "\"Allo?\"",
                "\"Qui parla?\"",
                "\"El torro de macho\"",
                "\"Boerno sirras senor\"",
                "\"No hipla no haplo\"",
                "\"E ley?\"",
                "\"Allo - diggaloo diggaley!\"",
                "\"Bello?\"",
                "\"Hvar?\"",
                "\"E eh - bello?\"",
                "\"Amiga callamiga caramello!\"",
                "\"Carmen\"",
                "\"Disco!\"",
                "\"Capisto\"",
                "\"Uh\"",
                "\"Uhhmmmm\""
            ]
        ]
    }
]