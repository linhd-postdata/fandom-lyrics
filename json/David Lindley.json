[
    {
        "author": "David Lindley",
        "title": "El Rayo-X",
        "text": [
            [
                "I am \"El Rayo\".",
                "Soy el cantar del gallo.",
                "Soy el caiman del bayou,",
                "Cuando lo parte el rayo."
            ],
            [
                "Ven, ven aca.",
                "Vuelve aca.",
                "No no tengas miedo.",
                "No no te voy a dar."
            ],
            [
                "Soy alacrán pelú,",
                "Que pican en el desierto.",
                "Cuando se va la luz?",
                "Ėl ha no mas el viento."
            ],
            [
                "Oye me flor del Mayo,",
                "Yo soy el hombre Rayo.",
                "Soy el cantar del gallo,",
                "Cuando lo parte el rayo."
            ],
            [
                "Ven, ven aca.",
                "Vuelve aca.",
                "No no tengas miedo.",
                "No no te voy a dar."
            ]
        ]
    }
]