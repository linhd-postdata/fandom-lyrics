[
    {
        "author": "Apostle Of Hustle",
        "title": "¡Rafaga!",
        "text": [
            [
                "Pasaba mi niña",
                "¡Qué bonita iba",
                "Con su vestidito",
                "De muselina",
                "Y una mariposa",
                "Prendida"
            ],
            [
                "¡Síguela, muchacho",
                "La vereda arriba",
                "Y si ves que llora",
                "O medita",
                "Píntale el corazón",
                "Con purpurina",
                "Y dile que no llore si queda solita"
            ]
        ]
    },
    {
        "author": "Apostle Of Hustle",
        "title": "Fast Pony for Victor Jara",
        "text": [
            [
                "Los frios traficantes",
                "De suenos en revistas",
                "Que en la juentud",
                "Engordan y profitan"
            ],
            [
                "Tocieron sus anhelos",
                "Y le dieron mentira",
                "La dicha embotellada",
                "Amor y fantasia"
            ]
        ]
    }
]