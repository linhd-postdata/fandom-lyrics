[
    {
        "author": "Askers",
        "title": "Siesta",
        "text": [
            [
                "Estan cerrando",
                "no se puede emborracharse",
                "yo no creo",
                "ya que puedo resistir",
                "y con esta noche adelante",
                "yo no quiero pararne",
                "Hasta manana"
            ],
            [
                "Y tu la sabe",
                "rue quiero",
                "resistencia y nada mas",
                "y tu lo sabes",
                "que quiero",
                "una siesta y nada mas"
            ],
            [
                "Esta cultura",
                "te prohibe de expresarte",
                "yo no creo",
                "ya que puedo resistir"
            ],
            [
                "Y tu la sabe",
                "rue quiero",
                "resistencia y nada mas",
                "y tu lo sabes",
                "que quiero",
                "una siesta y nada mas"
            ]
        ]
    }
]