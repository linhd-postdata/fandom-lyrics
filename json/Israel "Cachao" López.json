    {
        "author": "Israel \"Cachao\" López",
        "title": "Cuba Linda",
        "text": [
            [
                "Jose Deza, Autor",
                "Virgilio Marti, Cantante"
            ],
            [
                "Cuba linda de mi vida",
                "Cuba linda siempre te recordare",
                "Yo quiesera verte ahora",
                "Como la primera vez"
            ],
            [
                "Cuba linda de mi vida",
                "Cuba linda siempre te recordare",
                "Cuba linda siempre te recordare"
            ],
            [
                "Cuando escucho un son Cubano",
                "De los tiempos ya pasado",
                "Mi corazon se triteze"
            ],
            [
                "Y mi juventud revive",
                "Eso tesoro Cubano"
            ],
            [
                "Tierra de sueno y de encanto",
                "Con su son tan habanero",
                "Tierra que yo tanto quiero",
                "Que por ella yo me muero"
            ],
            [
                "Cuba linda de mi vida",
                "Cuba linda siempre te recordare"
            ],
            [
                "Te Recordaremos"
            ]
        ]
    }
]