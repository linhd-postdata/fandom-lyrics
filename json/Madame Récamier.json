[
    {
        "author": "Madame Récamier",
        "title": "Mira Mira",
        "text": [
            [
                "Cuando no queda mas",
                "Que hacer, por ti",
                "Busco en mi interior algo que me aga feliz",
                "No me engañes yo se que soy para ti",
                "Sobran las palabras mejor ya dejalo ir"
            ],
            [
                ":coro:",
                "Mira mira mira",
                "Con cuantas ganas,con cuantas ganas",
                "Me quieres ver"
            ],
            [
                "Mira mira mira",
                "Con cuantas ganas,con cuantas ganas",
                "Me quieres tener"
            ],
            [
                "Cuando no queda mas que hacer por ti",
                "Busco formas para aparecer por ahi",
                "Quiero que me encuentres otra vez",
                "Sin querer",
                "Se que te lastime lo tenia que hacer"
            ],
            [
                ":coro: (x2)"
            ],
            [
                "Mira mira mira",
                "Con cuantas ganas, con cuantas ganas",
                "Me quieres ver"
            ],
            [
                "Mira mira mira",
                "Con cuantas ganas, con cuantas ganas",
                "Me quieres tener"
            ]
        ]
    }
]