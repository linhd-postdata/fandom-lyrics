[
    {
        "author": "Mía Maestro",
        "title": "Lloverá",
        "text": [
            [
                "Llovera, gotas mínimas",
                "Lloverá, de mi boca,",
                "Saldrá el mar"
            ],
            [
                "No, no, Romeo, no,",
                "No, no, me dejes (ver)",
                "Tu vida fue gestada ya…",
                "Lloverá, caudales de agua,",
                "Agua lisa"
            ],
            [
                "Lloverá, desde mis ojos"
            ],
            [
                "No, no, Romeo, no,",
                "no, no me dejes ver",
                "que tu muerte fue gestada ya…",
                "Renacimiento"
            ]
        ]
    }
]