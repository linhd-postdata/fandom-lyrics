[
    {
        "author": "Vicci Martinez",
        "title": "Ganar Tu Amor",
        "text": [
            [
                "Los ojos claros, te puedo ver",
                "Lo que deseas decir, poder desaparecer",
                "Misterios y aventuras que tendrás",
                "Contigo siempre deseo estar…"
            ],
            [
                "Quisiera de tu amor gozar",
                "Cariños sin fin disfrutar",
                "Pero lo termine…"
            ],
            [
                "Lo hice desaparecer",
                "Tengo que volverlo a ver…",
                "Y Ganar tu amor"
            ],
            [
                "Ahora entiendeme",
                "El modo que sentí y lo que recibí",
                "Mi amor del pasado me hace reflejar",
                "De la locura que sufrí la tristeza que sentí…"
            ],
            [
                "Quisiera de tu amor gozar",
                "Cariños sin fin disfrutar",
                "Pero lo termine…"
            ],
            [
                "Lo hice desaparecer",
                "Tengo que volverlo a ver…",
                "Y Ganar tu amor",
                "Y Ganar tu amor"
            ]
        ]
    }
]