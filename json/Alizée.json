[
    {
        "author": "Alizée",
        "title": "La Candida",
        "text": [
            [
                "Yo aquí",
                "Tú allá",
                "Por mí volverás",
                "Me muero, me muero..."
            ],
            [
                "Lo sé eres fiel",
                "Jamás mentirás",
                "No olvidarás",
                "No hay otra más, a quien amarás ¡Si no es a mí!"
            ],
            [
                "¡Soy La Candida!",
                "Enamorada",
                "Yo creo en ti",
                "¡Soy La Candida!",
                "Tú no me engañas",
                "Piensas en mí."
            ],
            [
                "Lo sé eres fiel",
                "Jamás mentirás",
                "Me muero, me muero",
                "Feliz tú sin mí",
                "¡Oh Dios no podrás!",
                "Vives aquí..."
            ],
            [
                "Aquí, mira aquí",
                "Sólo para mí",
                "Yo creo en ti"
            ],
            [
                "¡Soy La Candida!",
                "Enamorada",
                "Piensas en mí",
                "¡Soy La Candida!",
                "Tú no me engañas",
                "Piensas en mí."
            ],
            [
                "¡Candida!"
            ]
        ]
    }
]