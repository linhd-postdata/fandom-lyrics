[
    {
        "author": "Bourbon Road",
        "title": "Manos Vacias",
        "text": [
            [
                "Me vas a echar de menos",
                "Si no me puedes ver.",
                "Me vas a echar de menos",
                "Si no me puedes ver.",
                "Que me voy por la mañana",
                "Para no volver."
            ],
            [
                "Me has dejado hecho polvo.",
                "Es culpa tuya que estas sola.",
                "Me has dejado hecho polvo.",
                "Es culpa tuya que estas sola.",
                "No me querias, chiquitita",
                "Hasta que se fue el otro."
            ],
            [
                "Puedes intentar ganarme",
                "Con tus trucos y engaños.",
                "Puedes intentar ganarme",
                "Con tus trucos y engaños.",
                "Pero cuando acaba todo,",
                "Vacias se quedaran tus manos (y sola estaras)"
            ]
        ]
    }
]