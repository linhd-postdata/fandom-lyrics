[
    {
        "author": "Madball",
        "title": "Mi Palabra",
        "text": [
            [
                "Muchas palabras sin acción",
                "Computadoras sin emoción",
                "Vive tu vida",
                "Sal del rincon",
                "Vive tus palabras",
                "Sin precaución"
            ],
            [
                "Existir",
                "No es vivir",
                "Mis palabras",
                "Yo las vivo si"
            ],
            [
                "Viven hablando de otros",
                "Sin saber de lo que hablan celosos",
                "Un dia van aprender",
                "Si que ladran peros",
                "Pero no muerden",
                "Mis palabras huesos",
                "Que aguantan mi piel",
                "Mi corazon me lleva",
                "Mis manos son de piedra y en esta selva",
                "Palabras solas no valen"
            ],
            [
                "Y las mias son de sinceridad",
                "No es un cuento",
                "Solo es la verdad"
            ],
            [
                "Existir",
                "No es vivir"
            ],
            [
                "Mis palabras yo las vivo si"
            ]
        ]
    }
]