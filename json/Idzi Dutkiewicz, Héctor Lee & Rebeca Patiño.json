[
    {
        "author": "Idzi Dutkiewicz, Héctor Lee & Rebeca Patiño",
        "title": "For The Dancing and the Dreaming",
        "text": [
            [
                "Estoico",
                "Por bravo mar navegaré",
                "Ahogarme yo no temo",
                "Y sortearé la tempestad",
                "Si eres para mí",
                "Ni aridente sol",
                "Ni frio atroz",
                "Me harán"
            ],
            [
                "Bocón",
                "Me harán dejar mi viaje",
                "Lo siento"
            ],
            [
                "Estoico",
                "Si me prometes, corazón",
                "Amar"
            ],
            [
                "Valka",
                "Amarme por la eternidad",
                "Mi buen amor, tan dulce y leal",
                "Me asombran tus palabras",
                "No quiero una empresa audaz",
                "Es bastante si me abrazas"
            ],
            [
                "Estoico",
                "Sortijas de oro te trareré",
                "Poemas te voy a cantar (Valka: ¿Lo harias?)",
                "Te cuidaré de todo mal",
                "Si siempre me acompañas"
            ],
            [
                "Valka",
                "Sortijas de oro para qué",
                "Poemas no me importan ya",
                "Tu mano solo sosetener"
            ],
            [
                "Estoico",
                "Mejor que eso, no hay más"
            ],
            [
                "Estoico & Valka",
                "Con tus abrazos y tu amor",
                "En las danzas y los sueños",
                "En pena y alegría igual",
                "Conmigo you te llevo",
                "Por bravo mar navegaré",
                "Ahogarme yo no temo",
                "Y sortearé la tempestad",
                "Si eres para mí"
            ],
            [
                "Bocón",
                "Míiiiiiiiiii...",
                "Sigo cantado!",
                "Ya no"
            ]
        ]
    }
]