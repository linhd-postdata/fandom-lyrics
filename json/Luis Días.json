[
    {
        "author": "Luis Días",
        "title": "Yo Quiero Andar",
        "text": [
            [
                "Quieres dormir",
                "Y yo quiero andar",
                "La noche es para un largo viaje",
                "Y hay que llegar"
            ],
            [
                "Tropecé ayer",
                "Con tu cariño",
                "Casi me 'tuve al quedar"
            ],
            [
                "Mi equipaje cargué conmigo",
                "Salí decidida a llegar"
            ],
            [
                "Y tú no sabrás"
            ],
            [
                "Donde encontrarme",
                "Donde molestarme",
                "Donde fastidiarme",
                "Donde tu buscarme",
                "Donde golpearme",
                "Donde abusarme",
                "Donde encontrarme",
                "Y molestarme"
            ],
            [
                "Quieres dormir",
                "Y yo quiero andar",
                "La noche es para un largo viaje",
                "Y hay que llegar"
            ],
            [
                "Tengo sed",
                "No tengo aliento",
                "Sólo me queda intentar"
            ],
            [
                "Déjame abrir mi pecho",
                "Huele a noche y huele a mar"
            ],
            [
                "No soñé lugar"
            ],
            [
                "Para tu gozarme",
                "Y abusarme",
                "Para tu encontrarme",
                "Y después dejarme",
                "Y siempre llamarme",
                "Y traicionarme",
                "Y ante todo el mundo",
                "Desconsiderarme"
            ],
            [
                "Quieres dormir...",
                "Y yo quiero andar!",
                "Y yo quiero andar!",
                "Y yo quiero andar!",
                "Y yo quiero andar!",
                "Quieres dormir...",
                "Y traicionarme",
                "Quieres dormir...",
                "Y después dejarme",
                "Quieres dormir...",
                "Y tú buscarme",
                "Quieres dormir...",
                "Y ante todo el mundo",
                "Desconsiderarme"
            ],
            [
                "Quieres dormir",
                "Y yo quiero andar!"
            ]
        ]
    }
]