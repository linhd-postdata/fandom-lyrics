[
    {
        "author": "Las Musas Del Vallenato",
        "title": "Declaración De Amor",
        "text": [
            [
                "Declaracion de amor"
            ],
            [
                "Tengo que decirte lo que siento",
                "Pues ya no puedo ocultarlo",
                "En mi pobre corazon."
            ],
            [
                "Quiero que tu sepas que te amo",
                "Que de ti me he enamorado",
                "Es una declaracion de amor"
            ],
            [
                "Hoy me siento decidida",
                "Al decirte que siento",
                "Te parecera muy raro",
                "Lo que estoy haciendo"
            ],
            [
                "Pero es que tu me gustas mucho",
                "Y a cada momento te busco",
                "Por eso quisiera que tu fueras dueño de mi corazon"
            ],
            [
                "Tengo que decirte lo que siento....",
                "(x2)"
            ],
            [
                "Hoy me siento decidida",
                "Al decirte que siento"
            ]
        ]
    }
]