[
    {
        "author": "La Féline",
        "title": "Piropos",
        "text": [
            [
                "Pirópos"
            ],
            [
                "Hasta que hablen las piedras",
                "Hasta que yo me haga vieja",
                "Hasta que me crezcan en el hombro",
                "Dos alitas y un moño",
                "Para ya con tus pirópos",
                "No quiero más",
                "No quiero más",
                "Para ya con tus pirópos",
                "No quiero más",
                "No quiero más",
                "No quiero, no quiero, no quiero, no quiero, no quiero, no quiero, no quiero más",
                "Hasta que llegue el diluvio",
                "Hasta que recomience el mundo",
                "Hasta que el verano ponga fín",
                "Hasta que llame tu nombre",
                "Para ya con tus pirópos",
                "No quiero más",
                "No quiero más",
                "Para ya con tus pirópos",
                "No quiero más",
                "No quiero más",
                "No quiero, no quiero, no quiero, no quiero, no quiero, no quiero, no quiero más"
            ]
        ]
    }
]