[
    {
        "author": "Nátura",
        "title": "Neurona",
        "text": [
            [
                "Es imposible distinguir",
                "Cuando la niebla cubre tu sentir",
                "Una neurona no puede decifrar",
                "Tal misterio, tal realidad"
            ],
            [
                "Es imposible tratar de resignar",
                "Nuestros instintos, la soledad",
                "Es tan inútil tratar de revivir",
                "Todo el pasado, este sufrir"
            ],
            [
                "Y ahora tengo compasión de ti",
                "Y ahora siento compasión de ti"
            ],
            [
                "Es imposible asimilar",
                "Cuando tu alma me hiere sin cesar",
                "Cuando tu espíritu, inquieto al despertar",
                "No me deja descansar"
            ],
            [
                "Es imposible tratar de contener",
                "Tu subconsciencia, sin poder vencer",
                "Una neurona no puede decifrar",
                "Tus sentimientos, tu fé sin profesar"
            ],
            [
                "Y ahora tengo compasión de ti",
                "Y ahora siento compasión de ti"
            ]
        ]
    },
    {
        "author": "Nátura",
        "title": "Si Yo Volara",
        "text": [
            [
                "¿Y tú qué has hecho",
                "Por tenerme a tu lado así?",
                "Si ves que ya no hay más",
                "Te empeñas en seguir"
            ],
            [
                "Si yo volara",
                "Amarrarías mis alas, callarías mi voz",
                "Si yo quisiera huir",
                "Si yo quisiera huir sin ti"
            ],
            [
                "Mataría por saber",
                "Moriría por querer",
                "Mataría por saber",
                "Moriría por querer"
            ],
            [
                "¿Y tú qué harías",
                "Por tenerme a tu lado así?",
                "¿Cambiarías tus fantasmas",
                "Por mi libertad?"
            ],
            [
                "Si tu miraras",
                "A los ojos de quien ya no está",
                "Nunca tendría que explicar",
                "Si ya no lloran más por ti"
            ],
            [
                "Mataría por saber",
                "Moriría por querer",
                "Mataría por saber",
                "Moriría por querer"
            ],
            [
                "Si yo volara",
                "Arrancarías mis alas, matarías mi voz",
                "Si yo quisiera huir",
                "Sin ti"
            ]
        ]
    }
]