                "De perfil resalta mucho",
                "Pero no me operare",
                "Si dios asi me la hizo",
                "Asi me la dejare"
            ],
            [
                "Hay que nariz",
                "Me fue a todar",
                "Pues como no",
                "Si soy Tucan"
            ],
            [
                "Soy narizon",
                "Que voy a hacer",
                "Lo que me pida",
                "Yo le dare"
            ],
            [
                "Se repite toda de nuevo"
            ]
        ]
