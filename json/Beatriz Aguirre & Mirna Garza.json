[
    {
        "author": "Beatriz Aguirre & Mirna Garza",
        "title": "Una Vez En Diciembre (Prólogo)",
        "text": [
            [
                "Marie:",
                "Piensa en mí siempre así",
                "Haz que el sueño recuerde"
            ],
            [
                "Marie y Anastasia:",
                "Tú vendrás junto a mí",
                "Cuando llegue diciembre"
            ]
        ]
    }
]