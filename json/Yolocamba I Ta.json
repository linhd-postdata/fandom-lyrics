[
    {
        "author": "Yolocamba I Ta",
        "title": "El Salvador Ta Venciendo",
        "text": [
            [
                "Por la calles viene un canto",
                "Anunciando amanecida"
            ],
            [
                "Al asalto compañeros",
                "Vamos muchachos",
                "Vamos pa' arriba (2 veces)"
            ],
            [
                "Fortalecer la unidad",
                "Garantiza la victoria"
            ],
            [
                "De un pueblo que se levanta",
                "Junto a la clase trabajadora. (2 veces)"
            ],
            [
                "A la carga compañeros",
                "Que aquí la guitarra grita",
                "El salvador ta venciendo",
                "Vamos muchachos",
                "Vamos pa' arriba (2 veces)"
            ],
            [
                "La bandera de los pobres",
                "Flamea en las barricadas"
            ],
            [
                "Afina la puntería",
                "La bestia gris ya va en desbandada (2 veces)"
            ],
            [
                "El Pueblo ya dijo basta",
                "La lucha es nuestra alegría"
            ],
            [
                "El futuro será nuestro",
                "Vamos muchachos",
                "Vamos pa' arriba. (2 veces)"
            ],
            [
                "A la carga compañeros",
                "Que aquí la guitarra grita",
                "El Salvador ta venciendo",
                "Vamos muchachos",
                "Vamos pa' arriba. (2 veces)"
            ],
            [
                "El Pueblo ya dijo basta",
                "La lucha es nuestra alegría"
            ],
            [
                "El futuro será nuestro",
                "Vamos muchachos",
                "Vamos pa' arriba. (2 veces)"
            ]
        ]
    }
]