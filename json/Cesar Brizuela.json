[
    {
        "author": "Cesar Brizuela",
        "title": "No Vas A Volver",
        "text": [
            [
                "Tragos amargo",
                "de infiel alegria",
                "una foto mas en mi coleccion",
                "que cuente mi vida"
            ],
            [
                "se esconde un disfras",
                "por lo que tu sientes",
                "dices que te vas",
                "y ami no me engana mi instinto valiente"
            ],
            [
                "no vas a volver",
                "te conosco bien",
                "te voy a perder"
            ],
            [
                "me resignare",
                "quieres libertad",
                "no te detendre"
            ],
            [
                "sabes, tirare",
                "ideas vacias",
                "absurdo sera seguir",
                "sin la musa de mis fantacias"
            ],
            [
                "la cama dira",
                "lo que fue alegria",
                "se hizo religion, en la habitacion",
                "amar cada dia"
            ],
            [
                "no vas a volver"
            ]
        ]
    }
]