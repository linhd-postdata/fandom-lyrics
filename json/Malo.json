[
    {
        "author": "Malo",
        "title": "Pana",
        "text": [
            [
                "Fumando mariguana fumandole",
                "tocandole mi pana tocandole",
                "mira mira ya",
                "la morena sin papa",
                "a donde a donde esta",
                "se fue con la chota",
                "mira mira ya",
                "la morena sin papa",
                "a donde a donde esta",
                "se fue con la chota",
                "se fue con la chota",
                "se fue con la chota",
                "voy a cantar voy a bailar",
                "voy a gozar hasta que llegue la chota"
            ]
        ]
    }
]