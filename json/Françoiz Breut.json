[
    {
        "author": "Françoiz Breut",
        "title": "Ciudad Del Mar",
        "text": [
            [
                "Ciudad del mar",
                "Cuando quieras volver",
                "Has las maletas y ven",
                "Nueva estación cambio de hora el reloj",
                "Lento de mi voluntad"
            ],
            [
                "Ciudad del mar, en tu playa gris",
                "Me vuelvo a esconder",
                "Ciudad del sol",
                "Donde apenas soy",
                "Una sombra de mi"
            ],
            [
                "Ciudad del mar",
                "Todo vuelve a nacer",
                "Pero tu quieres herir",
                "Mi orgullo otra vez",
                "Tanto placer te dan",
                "Tus pequenas vicotrias"
            ],
            [
                "Ciudad del mar, en tu playa gris",
                "Me vuelvo a esconder",
                "Ciudad del sol",
                "Donde apenas soy",
                "Una sombra de mi",
                "Ciudad del mar",
                "Oye el aire de ayer"
            ],
            [
                "El cielo tiembla como papel",
                "Nueva estacion, y no cambio et reloj",
                "Roto de tu deprecio"
            ]
        ]
    }
]