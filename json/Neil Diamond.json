    {
        "author": "Neil Diamond",
        "title": "Kol Nidre/My Name Is Yussel",
        "text": [
            [
                "Kol Nidre veesore vacharome",
                "V'konome v'chinuye v'kinuse",
                "Ushvu Ushvu os dindarno",
                "mi Yom Kippurim se ad Yom Kippurim"
            ]
        ]
    }
