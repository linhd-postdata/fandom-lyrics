[
    {
        "author": "Marka",
        "title": "Comida pa'la plebe",
        "text": [
            [
                "Aunque no quede",
                "Comida para la plebe",
                "A mi que se me dé",
                "Racion de canapes"
            ],
            [
                "Aunque no quede",
                "Comida para la plebe",
                "Aunque no quede nada",
                "A mi que más me dá"
            ],
            [
                "No comen bacalao, en el Sertao",
                "No comen pepeniyo, en Ermosiyo",
                "No comen jabali , la gente de Kigali",
                "No comen papafritas, los Vietnamitas"
            ],
            [
                "No beben cerveza, la gente de Fortaleza",
                "Nadie bebe zumo de manzána En La Habana",
                "No beben anis los Irakis",
                "En cuanto a los americanos,ni comen, ni ván al grano"
            ],
            [
                "",
                "Dame mas mas mas aceitunitas",
                "mas copitas",
                "Dame mas mas mas tapitas",
                "Asta que ya no pueda mas"
            ]
        ]
    }
]